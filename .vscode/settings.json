{
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "cmake.cmakePath": "/opt/homebrew/bin/cmake",
    "testMate.cpp.test.advancedExecutables": [
        {
            "pattern": "{build,Build,BUILD,out,Out,OUT}/Debug/**/msd_sim_test",
            "cwd": "${absDirpath}",
            "env": {
                "ExampleENV1": "You can use variables here too, like ${relPath}",
                "PATH": "${os_env:PATH}${osPathEnvSep}/adding/new/item/to/PATH/env"
            },
            "runTask": {
                "before": [
                    "CMake Build"
                ]
            },
            "testGrouping": {
                "groupByExecutable": {
                    "label": "MSD-Sim",
                    "groupByLabel": {
                        "label": "${sourceRelPath[3]}",
                        "groupByLabel": {
                            "label": "${sourceRelPath[4]}",
                        }
                    }
                }
            }
        },
        {
            "pattern": "{build,Build,BUILD,out,Out,OUT}/Debug/**/msd_gui_test",
            "cwd": "${absDirpath}",
            "env": {
                "ExampleENV1": "You can use variables here too, like ${relPath}",
                "PATH": "${os_env:PATH}${osPathEnvSep}/adding/new/item/to/PATH/env"
            },
            "runTask": {
                "before": [
                    "CMake Build"
                ]
            },
            "testGrouping": {
                "groupByExecutable": {
                    "label": "MSD-GUI",
                    "groupByLabel": {
                        "label": "${sourceRelPath[3]}",
                        "groupByLabel": {
                            "label": "${sourceRelPath[4]}",
                        }
                    }
                }
            }
        },
        {
            "pattern": "{build,Build,BUILD,out,Out,OUT}/Debug/**/msd_assets_test",
            "cwd": "${absDirpath}",
            "env": {
                "ExampleENV1": "You can use variables here too, like ${relPath}",
                "PATH": "${os_env:PATH}${osPathEnvSep}/adding/new/item/to/PATH/env"
            },
            "runTask": {
                "before": [
                    "CMake Build"
                ]
            },
            "testGrouping": {
                "groupByExecutable": {
                    "label": "MSD-Assets",
                    "groupByLabel": {
                        "label": "${sourceRelPath[3]}",
                        "groupByLabel": {
                            "label": "${sourceRelPath[4]}",
                        }
                    }
                }
            }
        },
        {
            "pattern": "{build,Build,BUILD,out,Out,OUT}/Debug/**/msd_utils_test",
            "cwd": "${absDirpath}",
            "env": {
                "ExampleENV1": "You can use variables here too, like ${relPath}",
                "PATH": "${os_env:PATH}${osPathEnvSep}/adding/new/item/to/PATH/env"
            },
            "runTask": {
                "before": [
                    "CMake Build"
                ]
            },
            "testGrouping": {
                "groupByExecutable": {
                    "label": "MSD-Utils",
                    "groupByLabel": {
                        "label": "${sourceRelPath[3]}",
                        "groupByLabel": {
                            "label": "${sourceRelPath[4]}",
                        }
                    }
                }
            }
        },
    ],
    "testMate.cpp.log.logpanel": true,
    "testMate.cpp.debug.configTemplate": {
        "type": "cppdbg",
        "MIMode": "lldb",
        "program": "${exec}",
        "args": "${argsArray}",
        "env": "${envObj}",
        "environment": "${envObjArray}",
        "sourceFileMap": "${sourceFileMapObj}",
        "setupCommands": [
            {
                "description": "Limit children to prevent hang on deep template types",
                "text": "settings set target.max-children-count 50",
                "ignoreFailures": true
            },
            // {
            //     "description": "Limit string summary length",
            //     "text": "settings set target.max-string-summary-length 100",
            //     "ignoreFailures": true
            // },
            // {
            //     "description": "Set memory read limit",
            //     "text": "settings set target.max-memory-read-size 512",
            //     "ignoreFailures": true
            // },
            {
                "description": "Disable type summaries to reduce template inspection",
                "text": "type summary clear",
                "ignoreFailures": true
            },
            {
                "description": "Set expression evaluation timeout",
                "text": "settings set target.expr-prefix-timeout 1000",
                "ignoreFailures": true
            }
        ],
        "externalConsole": false,
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}"
    },
    "cmake.options.statusBarVisibility": "visible",
    "cmake.ctest.testExplorerIntegrationEnabled": false,
    "files.associations": {
