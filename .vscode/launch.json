  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug msd_exe",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/Debug/debug/msd_exe",
      "args": [],
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "CMake Build",
      "stopAtEntry": false,
      "environment": [],
      "externalConsole": false,
      "MIMode": "lldb",
      "setupCommands": [
        {
          "description": "Load Eigen type formatters",
          "text": "command source ${workspaceFolder}/.lldbinit",
          "ignoreFailures": true
        },
        {
          "description": "Disable auto-loading of system formatters",
          "text": "settings set target.load-script-from-symbol-file false",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "Debug msd_exe (No Build)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/Debug/debug/msd_exe",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "environment": [],
      "externalConsole": false,
